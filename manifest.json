{
  "version": "1.0",
  "manifest_version": 2,
  "name": "Dark",
  "description": "A minimalistic dark theme for Google Chrome.",
  "theme": {
    "images": {
      "theme_frame": "images/frame.png",
      "theme_toolbar": "images/toolbar.png",
      "theme_tab_background": "images/tab.png"
    },
    "colors": {
      "frame": [0, 0, 0],
      "frame_inactive": [80, 80, 80],
      "frame_incognito": [0, 0, 80],
      "ntp_background": [0, 0, 0],
      "ntp_text": [200, 200, 200],
      "tab_text": [255, 255, 255],
      "tab_background_text": [160, 160, 160],
      "bookmark_text": [160, 160, 160],
      "toolbar": [0, 0, 0]
    }
  }
}
